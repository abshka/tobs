# Telegram API настройки (обязательные)
API_ID=your_api_id_here
API_HASH=your_api_hash_here
PHONE_NUMBER=your_phone_number_here

# Пути экспорта
EXPORT_PATH=./exports
CACHE_FILE=./tobs_cache.json

# Производительность
PERFORMANCE_PROFILE=balanced
WORKERS=8
DOWNLOAD_WORKERS=12
FFMPEG_WORKERS=4

# Медиа обработка
IMAGE_QUALITY=85
VIDEO_CRF=28
USE_H265=false

# Логирование
LOG_LEVEL=INFO

# Дополнительные настройки
EXPORT_COMMENTS=false
REQUEST_DELAY=0.5
DIALOG_FETCH_LIMIT=20

# Прокси (опционально)
# PROXY_TYPE=socks5
# PROXY_ADDR=127.0.0.1
# PROXY_PORT=1080
